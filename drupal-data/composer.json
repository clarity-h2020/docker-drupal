{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Pascal Dih√©",
            "role": "manager"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
	{
		"type": "composer",
		"url": "https://repository.scienceatwork.eu/drupal"
	}
    ],
    "require": {
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/admin_toolbar": "^1.24",
        "drupal/bootstrap": "^3.13",
        "drupal/chosen": "^2.6",
        "drupal/colorbox": "^1.4",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/console": "^1.8",
        "drupal/core": "~8.5.3",
        "drupal/corporate_lite": "^1.0",
        "drupal/ctools": "^3.0",
        "drupal/devel": "^1.2",
        "drupal/ds": "^3.1",
        "drupal/entity_browser": "^2.0",
        "drupal/features": "^3.7",
        "drupal/field_group": "^1.0",
        "drupal/file_entity": "^2.0@beta",
        "drupal/form_mode_manager": "^1.3",
        "drupal/geofield": "^1.0@beta",
        "drupal/geofield_map": "^2.6",
        "drupal/group": "^1.0@RC",
        "drupal/imce": "^1.6",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/json_field": "^1.0@RC",
        "drupal/jsonapi": "^1.23",
        "drupal/module_filter": "^3.1",
        "drupal/ng_lightbox": "^1.0@beta",
        "drupal/openlayersd8": "^1.1",
        "drupal/paragraphs": "^1.4",
        "drupal/pathauto": "^1.2",
        "drupal/prepopulate": "^2.0@alpha",
        "drupal/reactjs_mount": "^1.0@alpha",
        "drupal/restui": "^1.15",
        "drupal/showcase_lite": "^1.1",
        "drupal/smtp": "^1.0@beta",
        "drupal/twig_tweak": "^2.1",
        "drupal/view_mode_page": "^3.1",
        "drupal/viewfield": "^3.0@alpha",
        "drush/drush": "^9.0.0",
        "saw/saw_wizard": "0.9.0-dev",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "~8.5.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "secure-http": false
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
